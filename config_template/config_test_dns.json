            "mtu": 9000,
            "auto_route": true,
            "strict_route": true,
            "stack": "system",
            "endpoint_independent_nat": false,
            "sniff": false
        },
        {
            "listen": "0.0.0.0",
            "listen_port": 2080,
            "sniff": false,
            "tag": "mixed-in",
            "type": "mixed"
        }
    ],
    "outbounds": [
        {
            "tag": "VIEFAST VIP",
            "type": "selector",
            "outbounds": [
                "{all}"
            ]
        },
        {
            "type": "direct",
            "tag": "direct"
        },
        {
            "type": "dns",
            "tag": "dns-out"
        }
    ],
    "route": {
        "auto_detect_interface": true,
        "override_android_vpn": true,
        "final": "VIEFAST VIP",
        "rules": [
            {
                "outbound": "dns-out",
                "port": [
                    53
                ]
            }
