{
    "log": {
        "disabled": false,
        "level": "info",
        "timestamp": true
    },
    "experimental": {
        "clash_api": {
            "external_controller": "127.0.0.1:9090"
        }
    },
    "dns": {
        "servers": [
            {
                "tag": "proxy-dns",
                "address": "94.140.14.14",
