{
    "dns": {
        "independent_cache": true,
        "rules": [
            {
                "disable_cache": true,
                "outbound": "any",
                "server": "localDns"
            },
            {
                "query_type": [
                    "A",
                    "AAAA"
                ],
                "server": "remote"
            }
        ],
        "servers": [
            {
                "address": "8.8.8.8",
                "detour": "select",
                "tag": "proxyDns"
            },
            {
                "address": "local",
                "detour": "direct",
                "strategy": "ipv4_only",
                "tag": "localDns"
            },
            {
                "address": "rcode://success",
                "tag": "block"
            }
        ],
        "strategy": "ipv4_only"
    },
    "experimental": {
        "clash_api": {
            "external_controller": "127.0.0.1:9090"
        }
    },
    "inbounds": [
        {
            "auto_route": true,
            "endpoint_independent_nat": false,
            "inet4_address": "172.19.0.1/30",
            "interface_name": "tun0",
            "mtu": 1300,
            "sniff": false,
            "stack": "system",
            "strict_route": true,
            "tag": "tun-in",
            "type": "tun"
        },
        {
            "listen": "0.0.0.0",
            "listen_port": 2080,
            "sniff": false,
            "tag": "mixed-in",
            "type": "mixed"
        }
    ],
    "log": {
        "level": "info",
        "timestamp": true
    },
    "outbounds": [
        {
            "tag": "select",
            "type": "selector",
            "default": "urltest",
            "outbounds": [
                "{all}"
            ]
        },
        {
            "tag": "direct",
            "type": "direct"
        },
        {
            "tag": "block",
            "type": "block"
        },
        {
            "tag": "dns-out",
            "type": "dns"
        }
    ],
    "route": {
        "auto_detect_interface": true,
        "final": "select",
        "override_android_vpn": true,
        "rules": [
            {
                "outbound": "dns-out",
                "protocol": "dns"
            },
            {
                "inbound": [
                    "dns-in"
                ],
                "outbound": "dns-out"
            },
            {
                "outbound": "dns-out",
                "port": [
                    53
                ]
            },
            {
                "ip_cidr": [
                    "224.0.0.0/3",
                    "ff00::/8"
                ],
                "outbound": "block",
                "source_ip_cidr": [
                    "224.0.0.0/3",
                    "ff00::/8"
                ]
            }
        ]
    }
}